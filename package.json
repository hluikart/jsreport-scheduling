{
  "name": "jsreport-scheduling",
  "version": "1.3.4",
  "description": "jsreport extension for scheduling background rendering jobs",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "mocha test --timeout=10000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-scheduling",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-scheduling.git"
  },
  "keywords": [
    "jsreport",
    "scheduling"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.1",
    "cron": "1.3.0",
    "cron-builder": "0.3.0",
    "cronstrue": "0.10.10",
    "in-publish": "2.0.0",
    "moment": "2.19.1",
    "ordinal-number-suffix": "0.1.1",
    "shortid": "2.2.8",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.0.1",
    "eslint": "4.9.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "jsreport-core": "1.4.0",
    "jsreport-reports": "1.1.1",
    "jsreport-studio-dev": "1.1.1",
    "jsreport-templates": "1.1.1",
    "lodash": "4.17.4",
    "mocha": "4.0.1",
    "should": "13.1.2",
    "standard": "10.0.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
